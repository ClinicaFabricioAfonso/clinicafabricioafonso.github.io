{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Título",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Data",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "Description",
          "name": "excerpt",
          "type": "string"
        },
        {
          "title": "Categorias",
          "name": "categories",
          "type": "categories"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.content.pageFolders": [
    {
      "title": "Posts",
      "path": "[[workspace]]/_posts"
    }
  ],
  "frontMatter.taxonomy.tags": [],
  "frontMatter.taxonomy.categories": [
    "Alergia alimentar",
    "Asma e rinite",
    "Datas comemorativas",
    "Imunodeficiências",
    "Na mídia",
    "Notícias",
    "Orientações gerais",
    "Pele",
    "Reações a medicamentos"
  ],
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.framework.startCommand": "bundle exec jekyll s w"
}